{
  "name": "kaburlu-backend-v2",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && node dist/scripts/copy-assets.js",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --watch scripts --watch server.ts --ext ts,json --exec ts-node server.ts",
    "seed": "ts-node seed.ts",
    "openapi:generate": "ts-node scripts/generate-openapi.ts",
    "openapi:audit": "ts-node scripts/openapi-audit.ts",
    "smoke": "ts-node scripts/smoke-test.ts",
    "backfill:media": "ts-node src/scripts/backfillInfluencerAdMedia.ts",
    "update:media-status": "ts-node src/scripts/updateInfluencerAdMediaStatus.ts",
    "backfill:ad-pricing": "ts-node src/scripts/backfillAdPricing.ts",
    "backfill:pricing-table": "ts-node src/scripts/backfillInfluencerPricing.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@prisma/client": "^7.1.0",
    "@prisma/config": "^7.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.55.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "prisma": "^7.1.0",
    "sequelize": "^6.32.1",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.2",
    "@types/node-fetch": "^2.6.13",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
